# AWS Configuration
AWS_REGION=eu-west-3
AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}

# Database Configuration
DATABASE_URL=postgresql://autoaudit_app:${RDS_PASSWORD}@${RDS_ENDPOINT}:5432/autoaudit
DATABASE_URL_SYNC=postgresql+psycopg2://autoaudit_app:${RDS_PASSWORD}@${RDS_ENDPOINT}:5432/autoaudit
DB_PATH=/app/database/database.db

# Storage Configuration
S3_BUCKET=employee-matcher
S3_BUCKET_NAME=${APP_NAME}-uploads-${AWS_ACCOUNT_ID}
IMAGE_INPUT_DIR=/app/uploads/Images
PROCESSED_IMAGES_DIR=/app/Processed
EXCEL_OUTPUT_DIR=/app/output

# API Keys (these will be set as ECS Task environment variables)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=${LANGCHAIN_API_KEY}
OPENAI_API_KEY=${OPENAI_API_KEY}
WANDB_API_KEY=${WANDB_API_KEY}

# Server Configuration
SERVER_URL=https://api.autoauditpro.com
SERVER_PORT=8000
MAX_WORKERS=4
WORKERS_COUNT=4
BATCH_SIZE=10
LOG_LEVEL=INFO
MAX_CONCURRENT_WORKFLOWS=10

# Security
ACCESS_TOKEN_EXPIRE_MINUTES=30

# AWS Textract Configuration
TEXTRACT_CONFIDENCE_THRESHOLD=0.7

# Python Configuration
PYTHONUNBUFFERED=1 