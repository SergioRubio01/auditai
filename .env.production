# AWS Configuration
AWS_REGION=eu-west-3

# Database Configuration
DATABASE_URL=postgresql://autoaudit_app:${RDS_PASSWORD}@${RDS_ENDPOINT}:5432/autoaudit
REDIS_URL=redis://${ELASTICACHE_ENDPOINT}:6379

# Storage Configuration
S3_BUCKET=employee-matcher
IMAGE_INPUT_DIR=/tmp/uploads/Images

# API Keys (these will be set as ECS Task environment variables)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=${LANGCHAIN_API_KEY}
OPENAI_API_KEY=${OPENAI_API_KEY}
WANDB_API_KEY=${WANDB_API_KEY}

# Server Configuration
SERVER_URL=https://api.autoauditpro.com
MAX_WORKERS=4
BATCH_SIZE=10
LOG_LEVEL=INFO

# Security
NEXTAUTH_SECRET=${NEXTAUTH_SECRET}
ACCESS_TOKEN_EXPIRE_MINUTES=30
SYSTEM_INIT_KEY=${SYSTEM_INIT_KEY} 